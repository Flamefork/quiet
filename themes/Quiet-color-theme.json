{
  "name": "Quiet",
  "type": "light",
  "colors": {
    "activityBar.background": "#f0f0f0",
    "activityBar.foreground": "#0052b5",
    "activityBarBadge.background": "#0052b5",
    "editor.background": "#ffffff",
    "editor.findMatchBackground": "#bfdbfe",
    "editor.findMatchHighlightBackground": "#bfdbfe66",
    "editor.foreground": "#111111",
    "editor.lineHighlightBackground": "#f0f0f0",
    "editor.selectionBackground": "#bfdbfe",
    "editor.selectionHighlightBackground": "#bfdbfe66",
    "editorCursor.foreground": "#111111",
    "editorHoverWidget.background": "#f0f0f0",
    "editorHoverWidget.border": "#f0f0f0",
    "editorIndentGuide.activeBackground1": "#f0f0f0",
    "editorIndentGuide.background1": "#f0f0f0",
    "editorLineNumber.activeForeground": "#a0a0a0",
    "editorLineNumber.foreground": "#e0e0e0",
    "editorOverviewRuler.findMatchForeground": "#bfdbfe66",
    "editorRuler.foreground": "#f0f0f0",
    "editorSuggestWidget.background": "#f0f0f0",
    "editorSuggestWidget.border": "#f0f0f0",
    "focusBorder": "#bfdbfe",
    "gitDecoration.modifiedResourceForeground": "#0052b5",
    "list.activeSelectionBackground": "#bfdbfe",
    "list.activeSelectionForeground": "#111111",
    "panel.border": "#f0f0f0",
    "peekView.border": "#f0f0f0",
    "sideBar.background": "#ffffff",
    "sideBar.border": "#f0f0f0",
    "sideBarSectionHeader.background": "#ffffff",
    "sideBarSectionHeader.border": "#f0f0f0",
    "statusBar.background": "#f0f0f0",
    "statusBar.foreground": "#666666",
    "statusBarItem.remoteBackground": "#f0f0f0",
    "statusBarItem.remoteForeground": "#666666",
    "diffEditor.insertedLineBackground": "#9bb95511",
    "diffEditor.removedLineBackground": "#ff000011"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*.typeHint": "#666666",
    "type:go": "#666666",
    "namespace:go": "#666666",
    "method.declaration": {"fontStyle": "bold"},
    "function.declaration": {"fontStyle": "bold"},
    "function.declaration.local": {"fontStyle": ""},
    "variable.declaration": {"fontStyle": "bold"},
    "variable.declaration.local": {"fontStyle": ""},
    "variable.declaration:python": {"fontStyle": ""},
    "class.declaration": {"fontStyle": "bold"},
    "interface.declaration": {"fontStyle": "bold"},
    "type.definition:go": {"fontStyle": "bold"},
    "type.declaration": {"fontStyle": "bold"},
    "method.definition:go": {"fontStyle": "bold"},
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "foreground": "#a0a0a0"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#a0a0a0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "support.type.property-name",
        "meta.definition.variable",
      ],
      "settings": {
        "foreground": "#0052b5"
      }
    },
    {
      "name": "Types",
      "scope": [
        "meta.type.annotation",
      ],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#307070"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup: Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Markup: Header",
      "scope": [
        "punctuation.definition.heading",
        "markup.heading",
        "entity.name.type.instance.metadata"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "SQL: Text",
      "scope": [
        "meta.embedded.block.sql",
        "meta.embedded.sql",
        "meta.embedded.inline.sql"
      ],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "SQL: Tables",
      "scope": [
        "constant.other.database-name.sql",
        "constant.other.table-name.sql"
      ],
      "settings": {
        "foreground": "#0052b5"
      }
    }
  ]
}
