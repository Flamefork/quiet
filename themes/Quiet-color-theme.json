{
	"name": "Quiet",
	"type": "light",
	"colors": {
		"activityBar.background": "#f0f0f0",
		"activityBar.foreground": "#0052b5",
		"activityBarBadge.background": "#0052b5",
		"editor.background": "#ffffff",
		"editor.foreground": "#111111",
		"editor.lineHighlightBackground": "#f0f0f0",
		"editor.selectionBackground": "#bfdbfe",
		"editor.selectionHighlightBackground": "#bfdbfe66",
		"editorCursor.foreground": "#000000",
		"editorHoverWidget.background": "#ffffff",
		"editorHoverWidget.border": "#f0f0f0",
		"editorIndentGuide.activeBackground": "#f0f0f0",
		"editorIndentGuide.background": "#f0f0f0",
		"editorLineNumber.activeForeground": "#b0b0b0",
		"editorLineNumber.foreground": "#e0e0e0",
		"editorRuler.foreground": "#f0f0f0",
		"editorSuggestWidget.background": "#ffffff",
		"editorSuggestWidget.border": "#f0f0f0",
		"focusBorder": "#f0f0f0",
		"gitDecoration.modifiedResourceForeground": "#0052b5",
		"panel.border": "#f0f0f0",
		"sideBar.background": "#ffffff",
		"sideBar.border": "#f0f0f0",
		"sideBarSectionHeader.background": "#ffffff",
		"sideBarSectionHeader.border": "#f0f0f0",
		"statusBar.background": "#f0f0f0",
		"statusBar.foreground": "#666666",
        "editor.findMatchBackground": "#bfdbfe",
        "editor.findMatchHighlightBackground": "#bfdbfe",
        "editorOverviewRuler.findMatchForeground": "#bfdbfe",
        "peekView.border": "#f0f0f0",
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"entity.name.tag",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#0052b5"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
			],
			"settings": {
				"foreground": "#008080"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup: Code",
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Markup: Header",
			"scope": [
				"punctuation.definition.heading",
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold"
			}
		}
	]
}